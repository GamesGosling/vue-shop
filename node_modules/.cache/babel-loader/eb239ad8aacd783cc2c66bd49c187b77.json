iBAFA;AAGA,qBAAA,aAAA,GAAA,GAAA,CAAA,IAAA;AA/BA;AAAA;;AAAA;AAAA,sBAgCA,KAAA,WAAA,KAAA,GAhCA;AAAA;AAAA;AAAA;;AAAA;AAAA,uBAkCA,KAAA,KAAA,CAAA,GAAA,sBAAA,KAAA,MAAA,kBAAA;AAAA,kBAAA,MAAA,EACA;AACA,oBAAA,GAAA,EAAA;AADA;AADA,iBAAA,CAlCA;;AAAA;AAAA;AAkCA,gBAAA,IAlCA,SAkCA,IAlCA;;AAAA,sBAuCA,IAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAvCA;AAAA;AAAA;AAAA;;AAAA,kDAuCA,KAAA,QAAA,CAAA,KAAA,CAAA,UAAA,CAvCA;;AAAA;AAwCA,qBAAA,aAAA,GAAA,IAAA,CAAA,IAAA;;AAxCA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;;AAAA;AAAA;AAAA;;AAAA;AAAA;AA2CA;AACA,IAAA,aA5CA,yBA4CA,IA5CA,EA4CA;AACA,MAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AACA,WAAA,WAAA,GAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,GAAA;AACA,WAAA,oBAAA,GAAA,IAAA;AACA,KAhDA;AAiDA;AACA,IAAA,YAlDA,wBAkDA,IAlDA,EAkDA;AACA;AACA,UAAA,QAAA,GAAA,IAAA,CAAA,QAAA,CAAA,IAAA,CAAA,QAAA,CAFA,CAGA;;AACA,UAAA,CAAA,GAAA,KAAA,OAAA,CAAA,IAAA,CAAA,SAAA,CAAA,UAAA,CAAA;AAAA,eAAA,CAAA,CAAA,GAAA,KAAA,QAAA;AAAA,OAAA,CAAA,CAJA,CAKA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,MAAA,CAAA,CAAA,EAAA,CAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,QAAA;AACA,KA1DA;AA2DA;AACA,IAAA,aA5DA,yBA4DA,QA5DA,EA4DA;AACA;AACA,UAAA,OAAA,GAAA;AAAA,QAAA,GAAA,EAAA,QAAA,CAAA,IAAA,CAAA;AAAA,OAAA,CAFA,CAGA;;AACA,WAAA,OAAA,CAAA,IAAA,CAAA,IAAA,CAAA,OAAA;AACA,WAAA,QAAA,CAAA,OAAA,CAAA,QAAA;AACA,KAlEA;AAmEA;AACA,IAAA,QApEA,sBAoEA;AAAA;;AACA,WAAA,KAAA,CAAA,UAAA,CAAA,QAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gCAAA,kBAAA,KAAA;AAAA;;AAAA;AAAA;AAAA;AAAA;AAAA,uBACA,KADA;AAAA;AAAA;AAAA;;AAEA;AACA,kBAAA,KAAA,CAAA,aAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,wBAAA,OAAA,GAAA;AACA,sBAAA,OAAA,EAAA,IAAA,CAAA,OADA;AAEA,sBAAA,UAAA,EAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,GAAA;AAFA,qBAAA;;AAIA,oBAAA,KAAA,CAAA,OAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA,mBANA,EAHA,CAUA;;;AACA,kBAAA,KAAA,CAAA,aAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA;AACA,wBAAA,OAAA,GAAA;AACA,sBAAA,OAAA,EAAA,IAAA,CAAA,OADA;AAEA,sBAAA,UAAA,EAAA,IAAA,CAAA;AAFA,qBAAA;;AAIA,oBAAA,KAAA,CAAA,OAAA,CAAA,KAAA,CAAA,IAAA,CAAA,OAAA;AACA,mBANA,EAXA,CAkBA;AACA;;;AACA,kBAAA,IApBA,GAoBA,CAAA,CAAA,SAAA,CAAA,KAAA,CAAA,OAAA,CApBA;AAqBA,kBAAA,IAAA,CAAA,SAAA,GAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA,GAAA,CAAA;AACA,kBAAA,OAAA,CAAA,GAAA,CAAA,IAAA;AAtBA;AAAA,yBAuBA,KAAA,CAAA,KAAA,CAAA,IAAA,CAAA,QAAA,EAAA,IAAA,CAvBA;;AAAA;AAAA;AAuBA,kBAAA,GAvBA,SAuBA,IAvBA;;AAAA,wBAwBA,GAAA,CAAA,IAAA,CAAA,MAAA,KAAA,GAxBA;AAAA;AAAA;AAAA;;AAAA,oDAwBA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,QAAA,CAxBA;;AAAA;AAyBA,kBAAA,KAAA,CAAA,QAAA,CAAA,OAAA,CAAA,QAAA;;AAzBA;AAAA,yBA0BA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA,QAAA,CA1BA;;AAAA;AAAA;AAAA;;AAAA;AAAA,oDA4BA,KAAA,CAAA,QAAA,CAAA,KAAA,CAAA,YAAA,CA5BA;;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SAAA;;AAAA;AAAA;AAAA;AAAA;AA+BA;AApGA,GAzDA;AA+JA,EAAA,QAAA,EAAA;AACA,IAAA,MADA,oBACA;AACA,UAAA,KAAA,OAAA,CAAA,SAAA,CAAA,MAAA,KAAA,CAAA,EAAA;AACA,eAAA,KAAA,OAAA,CAAA,SAAA,CAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,eAAA,IAAA;AACA;AACA;AAPA;AA/JA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <!-- 面包屑导航区域 -->\r\n    <el-breadcrumb separator-class=\"el-icon-arrow-right\">\r\n      <el-breadcrumb-item :to=\"{ path: '/home' }\">首页</el-breadcrumb-item>\r\n      <el-breadcrumb-item>商品管理</el-breadcrumb-item>\r\n      <el-breadcrumb-item>添加商品</el-breadcrumb-item>\r\n    </el-breadcrumb>\r\n    <!-- 卡片 -->\r\n    <el-card>\r\n      <!-- 警告区域 -->\r\n      <el-alert\r\n        title=\"添加商品信息\"\r\n        type=\"info\"\r\n        center\r\n        show-icon\r\n        :closable=\"false\">\r\n      </el-alert>\r\n\r\n      <!-- 步骤条 -->\r\n      <el-steps :space=\"300\" :active=\"activeIndex - 0\" finish-status=\"success\" align-center center>\r\n        <el-step title=\"基本信息\"></el-step>\r\n        <el-step title=\"商品参数\"></el-step>\r\n        <el-step title=\"商品属性\"></el-step>\r\n        <el-step title=\"商品图片\"></el-step>\r\n        <el-step title=\"商品内容\"></el-step>\r\n        <el-step title=\"完成\"></el-step>\r\n      </el-steps>\r\n\r\n      <!-- tab标签页 -->\r\n      <el-form :model=\"addForm\" :rules=\"addFormRules\" ref=\"addFormRef\" label-width=\"100px\" label-position=\"top\">\r\n        <el-tabs v-model=\"activeIndex\" :tab-position=\"tabPosition\" :before-leave=\"beforeTabLeave\" @tab-click=\"tabClicked\">\r\n          <el-tab-pane label=\"基本信息\" name=\"0\">\r\n            <el-form-item label=\"商品名称\" prop=\"goods_name\">\r\n              <el-input v-model=\"addForm.goods_name\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"商品价格\" prop=\"goods_price\">\r\n              <el-input v-model=\"addForm.goods_price\" type=\"number\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"商品重量\" prop=\"goods_weight\">\r\n              <el-input v-model=\"addForm.goods_weight\" type=\"number\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"商品数量\" prop=\"goods_number\">\r\n              <el-input v-model=\"addForm.goods_number\" type=\"number\"></el-input>\r\n            </el-form-item>\r\n            <el-form-item label=\"商品分类\" prop=\"goods_cat\">\r\n              <el-cascader\r\n                v-model=\"addForm.goods_cat\"\r\n                :options=\"cateList\"\r\n                :props=\"cateProps\"\r\n                @change=\"handleChange\"\r\n                clearable></el-cascader>\r\n            </el-form-item>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"商品参数\" name=\"1\">\r\n            <!-- 渲染表单的item项 -->\r\n            <el-form-item v-for=\"item in manyTableData\" :key=\"item.attr_id\" :label=\"item.attr_name\">\r\n              <el-checkbox-group v-model=\"item.attr_vals\">\r\n                <el-checkbox :label=\"cb\" v-for=\"(cb, i) in item.attr_vals\" :key=\"i\" border></el-checkbox>\r\n              </el-checkbox-group>\r\n            </el-form-item>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"商品属性\" name=\"2\">\r\n            <el-form-item v-for=\"item in onlyTableData\" :key=\"item.attr_id\" :label=\"item.attr_name\">\r\n              <el-input v-model=\"item.attr_vals\"></el-input>\r\n            </el-form-item>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"商品图片\" name=\"3\">\r\n            <el-upload\r\n              :action=\"uploadURL\"\r\n              :on-preview=\"handlePreview\"\r\n              :on-remove=\"handleRemove\"\r\n              list-type=\"picture\"\r\n              :headers=\"headersObj\"\r\n              :on-success=\"handleSuccess\">\r\n              <el-button size=\"small\" type=\"primary\">点击上传</el-button>\r\n            </el-upload>\r\n          </el-tab-pane>\r\n          <el-tab-pane label=\"商品内容\" name=\"4\">\r\n            <!-- 富文本编辑器 -->\r\n            <quill-editor v-model=\"addForm.goods_introduce\"></quill-editor>\r\n            <!-- -->\r\n            <el-button type=\"primary\" class=\"btnAddGoods\" @click=\"addGoods\">添加商品</el-button>\r\n          </el-tab-pane>\r\n        </el-tabs>\r\n      </el-form>\r\n    </el-card>\r\n\r\n    <!-- 图片预览对话框 -->\r\n    <el-dialog\r\n      title=\"图片预览\"\r\n      :visible.sync=\"previewDialogVisible\"\r\n      width=\"50%\">\r\n      <img :src=\"previewPath\" class=\"previewImg\">\r\n    </el-dialog>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport _ from 'lodash'\r\nexport default {\r\n  name: 'add',\r\n  data() {\r\n    return {\r\n      activeIndex: '0',\r\n      tabPosition: 'left',\r\n      // 添加商品数据对象\r\n      addForm: {\r\n        goods_name: '',\r\n        goods_price: 0,\r\n        goods_weight: 0,\r\n        goods_number: 0,\r\n        goods_introduce: '',\r\n        // 商品所属的分类列表\r\n        goods_cat: [],\r\n        pics: [],\r\n        attrs: []\r\n      },\r\n      // 添加表单验证规则\r\n      addFormRules: {\r\n        goods_name: [\r\n          { required: true, message: '请输入商品名称', trigger: 'blur' }\r\n        ],\r\n        goods_price: [\r\n          { required: true, message: '请输入商品价格', trigger: 'blur' }\r\n        ],\r\n        goods_weight: [\r\n          { required: true, message: '请输入商品重量', trigger: 'blur' }\r\n        ],\r\n        goods_number: [\r\n          { required: true, message: '请输入商品数量', trigger: 'blur' }\r\n        ],\r\n        goods_cat: [\r\n          { required: true, message: '请选择商品分类', trigger: 'blur' }\r\n        ]\r\n      },\r\n      // 商品分类列表\r\n      cateList: [],\r\n      cateProps: {\r\n        expandTrigger: 'hover',\r\n        value: 'cat_id',\r\n        label: 'cat_name',\r\n        children: 'children'\r\n      },\r\n      manyTableData: [],\r\n      onlyTableData: [],\r\n      uploadURL: 'http://127.0.0.1:8888/api/private/v1/upload',\r\n      headersObj: {\r\n        Authorization: window.sessionStorage.getItem('token')\r\n      },\r\n      previewPath: '',\r\n      previewDialogVisible: false\r\n    }\r\n  },\r\n  created () {\r\n    this.getCateList()\r\n  },\r\n  methods: {\r\n    async getCateList() {\r\n      const { data: res } = await this.$http.get('/categories')\r\n      if (res.meta.status !== 200) return this.$message.error('获取商品分类数据失败')\r\n      this.cateList = res.data\r\n    },\r\n    // 级联选择器选择项发生变化时调用该函数\r\n    handleChange() {\r\n      console.log(this.addForm.goods_cat)\r\n      if (this.addForm.goods_cat.length !== 3) {\r\n        this.addForm.goods_cat = []\r\n      }\r\n    },\r\n    beforeTabLeave(activeName, oldActiveName) {\r\n      if (oldActiveName === '0' && this.addForm.goods_cat.length !== 3) {\r\n        this.$message.error('请选择商品分类')\r\n        return false\r\n      }\r\n    },\r\n    async tabClicked() {\r\n      if (this.activeIndex === '1') {\r\n        // 访问动态参数面板\r\n        const { data: res } = await this.$http.get(`categories/${this.cateId}/attributes`, { params:\r\n          {\r\n            sel: 'many'\r\n          }\r\n        })\r\n        if (res.meta.status !== 200) return this.$message.error('获取动态参数失败')\r\n        res.data.forEach(item => {\r\n          item.attr_vals = item.attr_vals.length === 0 ? [] : item.attr_vals.split(' ')\r\n        })\r\n        this.manyTableData = res.data\r\n      } else if (this.activeIndex === '2') {\r\n        // 访问静态属性面板\r\n        const { data: res } = await this.$http.get(`categories/${this.cateId}/attributes`, { params:\r\n            {\r\n              sel: 'only'\r\n            }\r\n        })\r\n        if (res.meta.status !== 200) return this.$message.error('获取静态属性失败')\r\n        this.onlyTableData = res.data\r\n      }\r\n    },\r\n    // 处理图片预览效果\r\n    handlePreview(file) {\r\n      console.log(file)\r\n      this.previewPath = file.response.data.url\r\n      this.previewDialogVisible = true\r\n    },\r\n    // 处理移除图片的操作\r\n    handleRemove(file) {\r\n      // 获取将要删除的图片的临时路径\r\n      const filePath = file.response.data.tmp_path\r\n      // 从pics数组中找到对应的索引\r\n      const i = this.addForm.pics.findIndex(x => (x.pic === filePath))\r\n      // 调用数组的splice方法，将图片信息从pics里移除\r\n      this.addForm.pics.splice(i, 1)\r\n      this.$message.success('删除图片成功')\r\n    },\r\n    // 监听上传图片成功的事件\r\n    handleSuccess(response) {\r\n      // 拼接得到图片信息对象\r\n      const picInfo = { pic: response.data.tmp_path }\r\n      // 将图片信息push到pics数组中\r\n      this.addForm.pics.push(picInfo)\r\n      this.$message.success('上传图片成功')\r\n    },\r\n    // 点击添加商品按钮事件\r\n    addGoods() {\r\n      this.$refs.addFormRef.validate(async valid => {\r\n        if (valid) {\r\n          // 处理manyTableData\r\n          this.manyTableData.forEach(item => {\r\n            const newInfo = {\r\n              attr_id: item.attr_id,\r\n              attr_value: item.attr_vals.join(' ')\r\n            }\r\n            this.addForm.attrs.push(newInfo)\r\n          })\r\n          // 处理onlyTableData\r\n          this.onlyTableData.forEach(item => {\r\n            const newInfo = {\r\n              attr_id: item.attr_id,\r\n              attr_value: item.attr_vals\r\n            }\r\n            this.addForm.attrs.push(newInfo)\r\n          })\r\n          // 处理goods_cat\r\n          // lodash cloneDeep(obj)\r\n          const form = _.cloneDeep(this.addForm)\r\n          form.goods_cat = form.goods_cat.join(',')\r\n          console.log(form)\r\n          const { data: res } = await this.$http.post('/goods', form)\r\n          if (res.meta.status !== 201) return this.$message.error('添加商品失败')\r\n          this.$message.success('添加商品成功')\r\n          await this.$router.push('/goods')\r\n        } else {\r\n          return this.$message.error('请填写必要的表单信息')\r\n        }\r\n      })\r\n    }\r\n  },\r\n  computed: {\r\n    cateId() {\r\n      if (this.addForm.goods_cat.length === 3) {\r\n        return this.addForm.goods_cat[2]\r\n      } else {\r\n        return null\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped lang=\"less\">\r\n  .el-checkbox{\r\n    margin: 0 10px 0 0 !important;\r\n  }\r\n  .previewImg{\r\n    width: 100%;\r\n  }\r\n  .btnAddGoods{\r\n    margin-top: 15px;\r\n  }\r\n</style>\r\n"],"sourceRoot":"src/components/goods"}]}